input two numbers num1 ,num2
input operation to be performed //op
//set result =0

if(op=='+')
   result= num1+num2
else if(op=='-')
   result= num1-num2
else if(op=='\')
   result= num1\num2
else if(op=='*')
   result= num1*num2
else if(op=='%')
   result= num1%num2

else 
  break



FIZZZBUZZZ

input number
if(number>=1  || number <=100)
{
if(number%3==0)
  print("Fizz")

else if(number%5==0)
   print("Buzz")
   
else if(number%3==0 && number%5==0)
     print("FizzBuzz")


}



pallindrome

input string //s
take the length of the string//len
set count=0;
for(i=0,j=len;i<j;i++,j--)
{

if(s[i]!=s[j])
 count++;
}

if(count==0)
print(pallindrome)

else
print(not  pallindrome)






largest from an integer array

input array//a[]
set largest =0
set n=num of elemnents in an array
for(i=0;i<n;i++)
{
if(a[i]>largest)
largest =a[i];

}
print(latgest)






 Sum of first 10 even numbers

begin
	declare sum as int
	declare i as int
	set i=0
	set sum=0
	set count = 0
	
	for(i=0;count<10;i++)
{
		if(i%2==0)
			sum=sum+i
			count=count+1
}	
	print sum



